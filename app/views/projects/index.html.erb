<div class="center hero-unit">
  <div class="row">
 
      <h2>
        <%= link_to "Add Project", new_project_path %>
      </h2>
      
      <table class="table table-hover">
           <thead>
              <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Code</th>
                <th>Description</th>
                <th>Bill Type</th>
                <th>Start Date</th>
                <th>Deadline</th>
                <th>End Date</th>
                <th>Giturl</th>
                <th>Status</th>
                <th>Actions</th>
              </tr>
            </thead>
        <% @project.each do |p| %>
          <tr>
            <td><%=p.id%></td>
            <td><%=p.name%></td>
            <td><%=p.code%></td>
            <td><%=p.description%></td>
            <td><%=p.billtype%></td>
            <td><%=p.startdate%></td>
            <td><%=p.deadline%></td>
            <td><%=p.enddate%></td>
            <td><%=p.giturl%></td>
            <td><%=p.status%></td>
            <td><%=link_to "V", project_path(p.id) %> <%=link_to "E", edit_project_path(p.id) %> <%=link_to "D",project_path(p.id) ,method: :delete %>
          </tr>
        <% end %>
    </table>
    
  </div>
</div>