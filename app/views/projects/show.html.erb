<% provide(:title, 'Project details') %>
<div>
  <div class = "row">
    <div class="span6 offset3">

      <%= form_for(@project) do |f| %>

        <%= f.label :name %>
        <%= f.text_field :name, :disabled => true%>

        <%= f.label :code %>
        <%= f.text_field :code, :disabled => true%>

        <%= f.label :description %>
        <%= f.text_field :description, :disabled => true %>

        <%= f.label :billtype %>
        <%= f.text_field :billtype, :disabled => true%>

